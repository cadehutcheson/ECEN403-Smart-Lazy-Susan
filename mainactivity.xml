<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:accessibilityLiveRegion="none"
    android:background="#873838"
    android:backgroundTint="#855151"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/ipAddr"
        android:layout_width="169dp"
        android:layout_height="52dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:text="@string/_192_168_1_168_21567"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.02" />

    <Button
        android:id="@+id/btn1"
        android:layout_width="65dp"
        android:layout_height="60dp"
        android:backgroundTint="#1E5553"
        android:text="@string/cw"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn2"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.016" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="65dp"
        android:layout_height="60dp"
        android:backgroundTint="#1E5553"
        android:text="@string/ccw"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.966"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.016" />

    <Button
        android:id="@+id/loadFoodBtn"
        android:layout_width="70dp"
        android:layout_height="48dp"
        android:layout_marginBottom="308dp"
        android:backgroundTint="#1E5553"
        android:text="@string/load_food_list"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/storeFoodBtn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/storeFoodBtn"
        app:layout_constraintTop_toBottomOf="@+id/storeFoodBtn"
        app:layout_constraintVertical_bias="0.042" />

    <EditText
        android:id="@+id/foodText"
        android:layout_width="153dp"
        android:layout_height="48dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        android:labelFor="@id/storeFoodBtn"
        android:minHeight="48dp"
        android:text="@string/item_name"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.038"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.106" />

    <Button
        android:id="@+id/storeFoodBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#1E5553"
        android:text="@string/store_to_list"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn1"
        app:layout_constraintTop_toTopOf="@+id/foodText"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/cmdBtn"
        style="@style/Widget.MaterialComponents.Button"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:backgroundTint="#1E5553"
        android:text="@string/go"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/SelectedFood"
        app:layout_constraintHorizontal_bias="0.962"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SelectedPos"
        app:layout_constraintVertical_bias="0.021" />

    <Button
        android:id="@+id/pos1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#1E5553"
        android:text="@string/pos_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.541"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.137" />

    <Button
        android:id="@+id/pos2"
        android:layout_width="69dp"
        android:layout_height="75dp"
        android:backgroundTint="#1E5553"
        android:text="@string/pos_2"
        app:layout_constraintBottom_toBottomOf="@+id/pos4"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.612"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/pos4"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/pos3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#1E5553"
        android:text="@string/pos_3"
        app:layout_constraintBottom_toTopOf="@+id/s3"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.983" />

    <Button
        android:id="@+id/pos4"
        android:layout_width="68dp"
        android:layout_height="75dp"

        android:backgroundTint="#1E5553"
        android:backgroundTintMode="src_in"
        android:text="@string/pos_4"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.471"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.507" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="229dp"
        android:layout_height="231dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.869"
        app:srcCompat="@drawable/square" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="197dp"
        android:layout_height="182dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:srcCompat="@drawable/circle" />

    <Button
        android:id="@+id/s1"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:backgroundTint="#855151"
        android:text="@string/_1"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.746" />

    <Button
        android:id="@+id/s2"
        android:layout_width="90dp"
        android:layout_height="48dp"
        android:backgroundTint="#855151"
        android:text="@string/_2"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <Button
        android:id="@+id/s3"
        android:layout_width="86dp"
        android:layout_height="48dp"
        android:backgroundTint="#855151"
        android:text="@string/_3"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.234" />

    <Button
        android:id="@+id/s4"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:backgroundTint="#855151"
        android:text="@string/_4"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.836"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/SelectedFood"
        android:layout_width="93dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:background="#F3E4E4"
        android:backgroundTint="#F3E4E4"
        android:text="@string/selectedFood"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.971"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loadFoodBtn" />

    <TextView
        android:id="@+id/SelectedPos"
        android:layout_width="90dp"
        android:layout_height="36dp"
        android:layout_marginTop="56dp"
        android:background="#F3E4E4"
        android:text="@string/selectedPos"
        app:layout_constraintEnd_toEndOf="@+id/SelectedFood"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/SelectedFood"
        app:layout_constraintTop_toBottomOf="@+id/loadFoodBtn" />

    <TextView
        android:id="@+id/menuList"
        android:layout_width="115dp"
        android:layout_height="166dp"
        android:background="#D5CDCD"
        android:text="@string/menuList"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/foodText"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/foodText"
        app:layout_constraintTop_toBottomOf="@+id/foodText"
        app:layout_constraintVertical_bias="0.044" />

    <Button
        android:id="@+id/deleteBtn"
        android:layout_width="75dp"
        android:layout_height="48dp"
        android:backgroundTint="#1E5553"
        android:text="@string/delete"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/loadFoodBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toEndOf="@+id/loadFoodBtn"
        app:layout_constraintTop_toTopOf="@+id/loadFoodBtn"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/placeBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:backgroundTint="#1E5553"
        android:text="Place"
        app:layout_constraintBottom_toBottomOf="@+id/menuList"
        app:layout_constraintEnd_toEndOf="@+id/menu"
        app:layout_constraintStart_toStartOf="@+id/menu"
        app:layout_constraintTop_toBottomOf="@+id/menu"
        app:layout_constraintVertical_bias="0.066" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/menu"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
        android:layout_width="121dp"
        android:layout_height="55dp"
        android:hint="@string/select"
        app:layout_constraintBottom_toBottomOf="@+id/SelectedFood"
        app:layout_constraintEnd_toStartOf="@+id/SelectedFood"
        app:layout_constraintHorizontal_bias="0.901"
        app:layout_constraintStart_toStartOf="@+id/menuList"
        app:layout_constraintTop_toTopOf="@+id/SelectedFood">

        <AutoCompleteTextView
            android:id="@+id/foodDropdown"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:inputType="none"
            tools:layout_editor_absoluteX="155dp"
            tools:layout_editor_absoluteY="207dp" />

    </com.google.android.material.textfield.TextInputLayout>


</androidx.constraintlayout.widget.ConstraintLayout>